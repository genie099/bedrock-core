{
  "name": "platform-web",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rm -rf ./dist && webpack -p",
    "start": "node ./serve",
    "static": "node ./serve-static",
    "theme:variables": "node scripts/update-theme-variables",
    "lint": "eslint src --fix",
    "test": "jest src",
    "lint:upgrade": "yarn upgrade eslint eslint-config-prettier prettier prettier-eslint eslint-plugin-react"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@kaareal/config": "^0.1.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-mobx-deep-action": "^1.6.0",
    "babel-plugin-styled-components": "1.5.1",
    "country-list": "^2.1.0",
    "css-loader": "^2.1.0",
    "exports-loader": "0.7.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "history": "4.7.2",
    "html-webpack-plugin": "3.2.0",
    "imports-loader": "0.8.0",
    "jest": "^24.7.1",
    "koa": "^2.6.2",
    "koa-compress": "3.0.0",
    "koa-static": "^5.0.0",
    "koa-webpack": "^5.2.1",
    "less": "2.7.3",
    "less-loader": "4.1.0",
    "lodash": "4.17.5",
    "lodash-webpack-plugin": "^0.11.5",
    "luxon": "^1.12.0",
    "mobx": "^5.8.0",
    "mobx-react": "5.2.8",
    "mobx-react-router": "^4.0.5",
    "raw-loader": "^1.0.0",
    "react": "^16.8.3",
    "react-day-picker": "^7.3.0",
    "react-dom": "^16.8.3",
    "react-dropzone": "^9.0.0",
    "react-hot-loader": "^4.7.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "semantic-ui-less": "^2.4.1",
    "semantic-ui-react": "^0.84.0",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.3",
    "url-loader": "1.0.1",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.12.3",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "prettier-eslint": "^8.8.2"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "legacyDecorators": true
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:jest/recommended",
      "prettier"
    ],
    "rules": {
      "react/display-name": 0,
      "react/prop-types": 0,
      "no-console": [
        "warn",
        {
          "allow": ["warn", "error", "info"]
        }
      ]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": ["node_modules", "src"]
        }
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always"
  },
  "toolchain": {
    "node": "10.15.1",
    "yarn": "1.13.0"
  }
}
